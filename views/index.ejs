<script>
         $('#btn').click(function(){
            $('.template-alert').html('')
       const email=$('#email').val();
       const pass=$('#pass').val();
       $.ajax({
            url:'/signin',
            type:'post',
            data:{
              email:email,
              password:pass
            }
          }).then(function(data){
          if (data.error) {
            return $('.template-alert').append(`
                 <div class="alert alert-danger" role="alert">
                      Tên tài khoản hoặc mật khẩu không chính xác
                 </div>
                 `)
          }
          if (!data.error) {
            if (!data.user.isActive) {
               return $('.template-alert').append(`
                 <div class="alert alert-danger" role="alert">
                      Tài khoản bạn đã bị khóa.Vui lòng liên hệ với tổng đài để được tư vấn
                 </div>
                 `)
            }
            if (data.user.isActive) {
                const id=data.user._id;
                localStorage.setItem('IDUser',JSON.stringify({id,role:data.user.role}));
                $('.template-alert').append(`
                 <div class="alert alert-success" role="alert">
                      Đăng nhập thành công
                 </div>
                 `)
                 setTimeout(function(){
                    window.location.href='/thongtin'
                },2000)
            }
            
               
            }      
           
          })
     })
          

</script>

