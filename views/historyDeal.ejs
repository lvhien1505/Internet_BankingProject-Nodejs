  <script>
    function getCookie(cname) {
      var name = cname + "=";
      var decodedCookie = decodeURIComponent(document.cookie);
      var ca = decodedCookie.split(";");
      for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == " ") {
          c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
          return c.substring(name.length, c.length);
        }
      }
      return "";
    }
    const token = getCookie("t");
    const data = JSON.parse(localStorage.getItem("IDUser"));
    if (!token||!data) {
        $(".mainContenTemplate").append(` <p align="center" class="textTitle">
                    <font color="FF0000">Bạn Phải Đăng Nhập Trước Khi Sử Dụng Chức Năng Này Xin Cảm Ơn...</font>
                </p>`);
    }
    if (data.role===1) {
        $('.template-info').append(`<li class="closed"><a href="/dashboard">Admin</a></li>`)
    }


$('.btn-tn').click(function(){
    $('.table-template').html('');
